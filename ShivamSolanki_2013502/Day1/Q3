#include<bits/stdc++.h>
using namespace std;
int main()
{int n;

cin>>n;
vector<int> a(n);
vector<int>ans;

for( int i=0;i<n;i++)
 {
    cin>>a[i];
 }
int i=0,j=n/2;
for( int k=0;k<n/2;k++)
{
   ans.push_back(a[i+k]);
   ans.push_back(a[j+k]);

}
for( int i=0;i<n;i++)
{
    cout<<endl<<ans[i];
}

}
